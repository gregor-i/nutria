{
  "dependencies": {
    "blockly": "^3.20200123.1",
    "browserify": "^16",
    "bulma": "^0.8",
    "clean-css-cli": "^4",
    "font-awesome": "^4",
    "sass": "^1.24.0",
    "snabbdom": "^0.7"
  },
  "scripts": {
    "build": "npm run cp && npm run build-css",
    "build-css": "sass frontend/src/main/css/nutria.sass service/public/css/nutria.css --no-source-map  --style compressed",
    "cp": "npm run cp-fas",
    "cp-fas": "cp node_modules/font-awesome/css/font-awesome.min.css service/public/css/font-awesome.css && cp -r node_modules/font-awesome/fonts service/public"
  }
}
