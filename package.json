{
  "dependencies": {
    "browserify": "^16",
    "bulma": "^0.8.2",
    "clean-css-cli": "^4",
    "font-awesome": "^4",
    "sass": "^1",
    "snabbdom": "^0.7"
  },
  "scripts": {
    "build": "npm run make-dirs && npm run cp && npm run build-css",
    "make-dirs": "mkdir -p backend/public/css && mkdir -p backend/public/js",
    "build-css": "sass frontend/src/main/css/nutria.sass backend/public/css/nutria.css --no-source-map  --style compressed",
    "cp": "npm run cp-fas",
    "cp-fas": "cp node_modules/font-awesome/css/font-awesome.min.css backend/public/css/font-awesome.css && cp -r node_modules/font-awesome/fonts backend/public"
  },
  "devDependencies": {
    "gl": "^4.5.0",
    "html-formatter": "^0",
    "jimp": "^0.10.1",
    "snabbdom-to-html": "^5"
  },
  "engines": {
    "node": ">= 12.16"
  }
}
